local function TTHIEOQ_fake_script() -- JumpToggle.Handler 
	local script = Instance.new('LocalScript', JumpToggle)

	_G.JumpToggle = false
	local Button = script.Parent
	local MagsText = script.Parent.Parent.JumpPower
	local TweenService = game:GetService("TweenService")
	local TweenSpeed = 0.30
	local TweenFunction = TweenInfo.new(TweenSpeed,Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0)
	
	
	Button.MouseButton1Click:Connect(function()
		if _G.JumpToggle == true then
			local Color1 = Color3.fromRGB(52, 52, 52)
			local Color2 = Color3.fromRGB(200,200,200)
		
			TweenService:Create(Button.Color, TweenFunction, {ImageColor3 = Color1}):Play()
			TweenService:Create(MagsText, TweenFunction, {TextColor3 = Color2}):Play()
			TweenService:Create(script.Parent.Parent.JumpSlider.Button.Color, TweenFunction, {ImageTransparency = 1}):Play()
			script.Parent.Parent.JumpSlider.JumpAmount.Visible = false
			_G.JumpToggle = false
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
			wait(1)
		else
			local Color = Color3.fromRGB(30,129,206)
			TweenService:Create(Button.Color, TweenFunction, {ImageColor3 = Color}):Play()
			TweenService:Create(MagsText, TweenFunction, {TextColor3 = Color}):Play()
			TweenService:Create(script.Parent.Parent.JumpSlider.Button.Color, TweenFunction, {ImageTransparency = 0}):Play()
			script.Parent.Parent.JumpSlider.JumpAmount.Visible = true
			_G.JumpToggle = true
			wait(1)
			
		end
		
	end)
	
	local Player = game:GetService'Players'.LocalPlayer;
	local UIS = game:GetService'UserInputService';
	local canjump = true
	
	game.Players.LocalPlayer.Character.Humanoid.StateChanged:Connect(function(_oldState, newState)
		if newState == Enum.HumanoidStateType.PlatformStanding or newState == Enum.HumanoidStateType.Freefall then
			canjump = false
		else
			canjump = true
	
		end
	
	end)
	
	UIS.InputBegan:connect(function(UserInput, gme)
		if gme then return end
		if UserInput.KeyCode == Enum.KeyCode.Space then
			if canjump == true and _G.JumpToggle == true then
				game.Players.LocalPlayer.Character.Humanoid.JumpPower = 0 
				game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity + Vector3.new(0, _G.JumpHeight, 0)
			end
		end
	end)
	
end
coroutine.wrap(TTHIEOQ_fake_script)()
