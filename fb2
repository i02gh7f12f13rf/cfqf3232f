local function GEWDZZ_fake_script() -- MagSlider1.Handler 
	local script = Instance.new('LocalScript', MagSlider1)

	local Slider = script.Parent
	local SliderBtn = Slider.Button
	local Player = game:GetService("Players").LocalPlayer
	local UIS = game:GetService("UserInputService")
	local RuS = game:GetService("RunService")
	
	-- Properties
	local held = false
	local step = 0.01
	_G.percentage = 0
	
	function snap(number, factor)
		if factor == 0 then
			return number
		else
			return math.floor(number/factor+0.5)*factor
		end
	end
	
	UIS.InputEnded:connect(function(input, processed)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			held = false
		end
	end)
	
	SliderBtn.MouseButton1Down:connect(function()
		held = true
	end)
	
	RuS.RenderStepped:connect(function(delta)
		if held then
			local MousePos = UIS:GetMouseLocation().X
			local BtnPos = SliderBtn.Position
			local SliderSize = Slider.AbsoluteSize.X
			local SliderPos = Slider.AbsolutePosition.X
			local pos = snap((MousePos-SliderPos)/SliderSize,step)
			_G.percentage = math.clamp(pos,0,0.87)
			SliderBtn.Position = UDim2.new(_G.percentage,0,BtnPos.Y.Scale, BtnPos.Y.Offset)
			script.Parent.MagsAmount.Text = math.round(_G.percentage * 100)
			_G.Magrange = _G.percentage * 100
			
		end
	end)
	
	
end
coroutine.wrap(GEWDZZ_fake_script)()
local function WNDWON_fake_script() -- MagsToggle.Handler 
	local script = Instance.new('LocalScript', MagsToggle)

	_G.MagToggle = false
	local Button = script.Parent
	local MagsText = script.Parent.Parent.MagsText
	local TweenService = game:GetService("TweenService")
	local TweenSpeed = 0.30
	local TweenFunction = TweenInfo.new(TweenSpeed,Enum.EasingStyle.Linear, Enum.EasingDirection.Out, 0)
	
	
	Button.MouseButton1Click:Connect(function()
		if _G.MagToggle == true then
			local Color1 = Color3.fromRGB(52, 52, 52)
			local Color2 = Color3.fromRGB(200,200,200)
		
			TweenService:Create(Button.Color, TweenFunction, {ImageColor3 = Color1}):Play()
			TweenService:Create(MagsText, TweenFunction, {TextColor3 = Color2}):Play()
			TweenService:Create(script.Parent.Parent.MagSlider1.Button.Color, TweenFunction, {ImageTransparency = 1}):Play()
			TweenService:Create(script.Parent.Parent.MagSlider2.Button.Color, TweenFunction, {ImageTransparency = 1}):Play()
			script.Parent.Parent.MagSlider1.MagsAmount.Visible = false
			script.Parent.Parent.MagSlider2.MagsAmount.Visible = false
			_G.MagToggle = false
			wait(1)
		else
			local Color = Color3.fromRGB(30,129,206)
			TweenService:Create(Button.Color, TweenFunction, {ImageColor3 = Color}):Play()
			TweenService:Create(MagsText, TweenFunction, {TextColor3 = Color}):Play()
			TweenService:Create(script.Parent.Parent.MagSlider1.Button.Color, TweenFunction, {ImageTransparency = 0}):Play()
			TweenService:Create(script.Parent.Parent.MagSlider2.Button.Color, TweenFunction, {ImageTransparency = 0}):Play()
			script.Parent.Parent.MagSlider1.MagsAmount.Visible = true
			script.Parent.Parent.MagSlider2.MagsAmount.Visible = true
			_G.MagToggle = true
			wait(1)
			_G.Magrange = _G.percentage * 100
			_G.Scale = 3
	
	
			while _G.MagToggle == true do
				for i,v in pairs(game.Workspace:GetChildren()) do
					if v.Name == "Football" then
	
						if v.ClassName == "Part" then
							UpperTorso = game.Players.LocalPlayer.Character.Torso
							local Calc = (v.Position-UpperTorso.Position).magnitude
							local Distance = math.floor(Calc)
							dist = Distance
						end
						if dist < (_G.Magrange) then
							if v.Parent == game.Players.LocalPlayer.Character or v.Parent ~= game.Workspace or v.ClassName == "Tool"  then
								v = nil
							else
								local ball = v
								if ball:FindFirstChild("Trail") then
									if ball.Trail.Enabled == true then
	
	
										ball.Position = game.Players.LocalPlayer.Character.CatchLeft.Position + Vector3.new(0,_G.Scale,0)
										ball.Position = game.Players.LocalPlayer.Character.CatchRight.Position + Vector3.new(0,_G.Scale,0)
									else
	
										ball = nil
									end
	
								end
	
							end
							if dist > (_G.Magrange) then 
	
							end     
	
						else
	
						end
					end
				end
				wait(0)
			end
		end
		
	end)
	
	
end
coroutine.wrap(WNDWON_fake_script)()
